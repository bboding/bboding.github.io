{
  "extends": "airbnb-base",
  "rules": {
    "semi": [2, "never"],
    "no-console": "off",
    "arrow-parens": "off",
    "implicit-arrow-linebreak": "off",
    "prefer-destructuring": "off",
    "camelcase": "off",
    "no-plusplus": "off",
    "no-await-in-loop": "off",
    "array-callback-return": "off",
    "no-restricted-syntax": "off",
    "import/prefer-default-export": "off",
    "no-new": "off",
    "import/extensions": "off"
  }
}
